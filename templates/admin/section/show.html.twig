{# templates/admin/section/show.html.twig -- Show a section #}

{% extends 'admin/base.html.twig' %}

{% block title %}Membre : {{ controller_name }}{% endblock %}

{% block body %}
    <h1>Section</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ section.id }}</td>
            </tr>
            <tr>
                <th>ContentFr</th>
                <td>{{ section.contentFr }}</td>
            </tr>
            <tr>
                <th>ContentNl</th>
                <td>{{ section.contentNl }}</td>
            </tr>
            <tr>
                <th>ContentEn</th>
                <td>{{ section.contentEn }}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}
